{
  "name": "flogo-amqp",
  "type": "flogo:trigger",
  "ref": "github.com/mtorre-iot/flogo-contrib/trigger/amqp",
  "version": "0.0.1",
  "title": "Receive AMQP Message",
  "author": "Mario Torre <mtorre.work@gmail.com>",
  "description": "Simple AMQP Trigger",
  "homepage": "https://github.com/mtorre-iot/flogo-contrib/tree/master/trigger/amqp",
  "settings":[
    {
      "name": "broker",
      "type": "string",
      "required": true
    },
    {
      "name": "id",
      "type": "string",
      "required": true
    },
    {
      "name": "user",
      "type": "string"
    },
    {
      "name": "password",
      "type": "string"
    },
    {
      "name": "store",
      "type": "string"
    },
    {
      "name": "qos",
      "type": "double"
    },
    {
      "name": "cleansess",
      "type": "boolean"
    }
  ],
  "output": [
    {
      "name": "message",
      "type": "string"
    }
  ],
  "reply": [
    {
      "name": "data",
      "type": "object"
    }
  ],
  "handler": {
    "settings": [
      {
        "name": "topic",
        "type": "string"
      }
    ]
  }
}
